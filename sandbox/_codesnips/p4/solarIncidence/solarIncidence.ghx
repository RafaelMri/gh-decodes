                    <item name="CodeInput" type_name="gh_string" type_code="10">import math
import decodes.extensions.solarGeom as sg


# gathering user input #
mymesh = mesh_in
radVals = radiation
colorA = low_color
colorB = high_color
hair_length_min = hair_length.Min
hair_length_max = hair_length.Max

# find the normals for each mesh face
normals = [Ray(mymesh.face_centroid(i),mymesh.face_normal(i)) for i in range(len(mymesh.faces))]

#visualize results by producing hairs and colors for each mesh face
colors = []
for n, normal in enumerate(normals):
    t = (radVals[n]-min(radVals))/(max(radVals)-min(radVals))
    colors.append(Color.interpolate(colorA,colorB,t))
    normal.vec.length = ((hair_length_max - hair_length_min) * t) + hair_length_min
    
hairs_out.put(normals)
colors_out.put(colors)




