                    <item name="CodeInput" type_name="gh_string" type_code="10">sl = starting_line
cp = charged_points
iterations = 200
step_size = 1.0
ppos = [sl.spt]
pvec = sl.vec.normalized(step_size)
threshold = step_size/2
keep_going = True

while keep_going:
    for cp in charged_points:
        # first calculate all the vecs that will influence the pvec
        cvec = Vec(ppos[-1],cp) # Get the vector for origin point and charge
        cvec.length = charges / cvec.length  # resize according to distance from particle
        pvec = (pvec + cvec).normalized() # alter the pvec
    
    # move the partilce
    ppos.append(ppos[-1]+pvec) # Move pt with current vector
    
    # test to see if we should stop
    for cp in charged_points:
        if cp.distance(ppos[-1]) &lt; threshold: keep_going = False # 1. too close to the charge
    if len(ppos) &gt; iterations : keep_going = False # 2. too many steps
    
for n in range(len(ppos)-1): a.put(Segment(ppos[n], ppos[n+1]))</item>
