class Bird():
    
    def __init__(self, initial_state, pos_start = None, vel_start = None):
        if pos_start is None : pos_start = Point.random(constrain2d=True)
        if vel_start is None : vel_start = Vec.random(constrain2d=True)
        
        # pos represents this bird's position
        self.pos = pos_start 
        # vel represents this bird's velocity and maximum velocity
        self.vel = vel_start 
        self.max_vel = 0.5
        # acl represents this bird's acceleration
        # this vector will be added to velocity at each step, 
        # so a acl of zero length will result in the bird maintaing its current
        # speed and direction
        self.acl = Vec() # initial acceleration is zero
        self.max_frc = 0.25 # the maximum force that any steering behavior can exert on the acceleration.
        
        self.is_alive = True # if false, stop recording steps
        self.clear_history() # initalize history lists 
        self.record_step() # record this initial condition as a "step"
        
        
        
    def step(self):
        """
        completes a simulation step for this bird.
        """
        if self.is_alive:
            self.vel += self.acl # add the acceleration to the velocity
            self.vel = self.vel.limited(self.max_vel) # limit to the maximum velocity
            
            self.record_step() # record the step (note that we record before moving)
            self.pos += self.vel # move bird
            self.acl = Vec() # reset acceleration back to zero for next step
        
    def steer(self, vec):
        """
        steers the bird by adding the given vector to the acceleration vector
        steer each bird before 'stepping' him within each simulation loop step
        """
        vec = vec.limited(self.max_frc) # limit to the maximum acceleration
        self.acl += vec
        
        
    def kill(self): self.is_alive = False
        
    # History Functions
    def reset(self):
        """
        resets this bird back to his initialized condition.
        replaces existing history lists with single inital values
        """
        self.is_alive = True
        if len(self.hist_pos) &gt; 0:
            self.pos = self.hist_pos[0]
            self.vel = self.hist_vel[0]
            self.acl = self.hist_acl[0]
        self.clear_history()
        self.record_step()
        
        
    def clear_history(self):
        """
        initializes history lists
        """
        self.hist_pos = []
        self.hist_vel = []
        self.hist_acl = []
        
    def record_step(self):
        """
        records the current state of this bird to history lists
        """
        if self.is_alive:
            self.hist_pos.append(self.pos)
            self.hist_vel.append(self.vel)
            self.hist_acl.append(self.acl)

