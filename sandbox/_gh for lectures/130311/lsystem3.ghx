        self.srch = search
        self.transform = func
        
    def matches(self,turtle):
        if turtle.name == self.srch : return True
        return False
        
        

def func(parent_cs): 
    opt = parent_cs.origin + parent_cs.xAxis
    x_axis = parent_cs.xAxis
    y_axis = parent_cs.yAxis
    return CS(opt,x_axis,y_axis)
a.put(MovementRule("a",func))

def func(parent_cs): 
    opt = parent_cs.origin + parent_cs.yAxis
    x_axis = parent_cs.yAxis
    y_axis = parent_cs.xAxis
    return CS(opt,x_axis,y_axis)
