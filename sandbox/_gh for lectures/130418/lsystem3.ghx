                    <item name="CodeInput" type_name="gh_string" type_code="10">class MovementRule():
    def __init__(self, search, func):
        self.srch = search
        self.transform = func
        
    def matches(self,turtle):
        if turtle.name == self.srch : return True
        return False
        
        

def func(pcs): 
    opt = pcs.eval(vec_a)
    x_axis = Vec(pcs.origin,pcs.eval(vec_a))
    y_axis = pcs.yAxis
    return CS(opt,x_axis,y_axis)
a.put(MovementRule("a",func))

def func(pcs): 
    opt = pcs.eval(vec_b)
    x_axis = Vec(pcs.origin,pcs.eval(vec_b))
    y_axis = pcs.zAxis
    return CS(opt,x_axis,y_axis)
a.put(MovementRule("b",func))

