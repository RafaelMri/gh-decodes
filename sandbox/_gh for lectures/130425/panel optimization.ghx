class QPanel(PGon):
    """
    a flat quad panel
    """
    
    def __init__(self,pts,indx):
        if len(pts) != 4 : raise Exception("four points please!")
        super(QPanel,self).__init__() #calls the PGon constructor
        self.basis = CS(pts[0],Vec(pts[0],pts[1]),Vec(pts[0],pts[3])) # find the cs of this panel
        
        # transform each of the given points into the 'space' of the cs
        xf = Xform.change_basis(self.basis,CS())
        pts = [pt*xf for pt in pts]
        if pts[2]._z != 0 : 
            print "non flat panel!  adjusting corner by ",pts[2].z
            pts[2]._z = 0
        
        self.append(pts)
        self.index = indx
        
    def is_similar_to(other):
        #TODO: compare this panel's corners with the other panel corners,
        # if w/in tolerence return true
        return False
        
        
    @property
    def do_translate(self): return False
        

a.put(QPanel)

if len(test_pts) == 4:
    pnl = QPanel(test_pts)
    b.put(pnl)
    b.put(pnl.basis_stripped())
