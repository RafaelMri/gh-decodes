                    <item name="CodeInput" type_name="gh_string" type_code="10">

msh = Mesh()
for v in v_vals:
    for u in u_vals:
        msh.append(func(u,v))

res_u = len(u_vals)
# simple triangulation style
for v in range(len(v_vals)):
    row = v*res_u
    for u in range(len(u_vals)-1):
        pi_0 = row+u
        pi_1 = row+u+1
        pi_2 = row+u+res_u+1
        pi_3 = row+u+res_u
        msh.add_face(pi_0,pi_1,pi_2,pi_3)

a.put(msh)
