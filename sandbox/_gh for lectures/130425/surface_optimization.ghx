                    <item name="CodeInput" type_name="gh_string" type_code="10">import math

def func(u,v):
    flat_pts = [Point(p.x,0,p.z) for p in pts]# ensure points lie on same plane as rotational axis by setting y=0
    
    angle = Interval.twopi().eval(u)
    xf = Xform.rotation(axis=Vec(0,0,1),angle=angle)
    rot_pts = [pt*xf for pt in flat_pts]
    
    crv = Curve.bezier(rot_pts)
    
    return crv.eval(v)
    

a.put(func)</item>
