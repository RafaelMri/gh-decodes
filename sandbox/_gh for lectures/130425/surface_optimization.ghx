import math

def facade_and_floor_area(func, res=10):
    circumferences = []
    floor_areas = []
    for ival in Interval()//res:
        pt_a = func(0.5,ival.a).origin
        pt_b = func(0.5,ival.b).origin
        cpt = Point(0,0,pt_a.z)
        radius = cpt.distance(pt_a)
        circ = math.pi * 2 * radius
        circumferences.append(circ * (pt_b.z - pt_a.z) )
        floor_areas.append(math.pi * radius**2) 
    fac_area = sum(circumferences)
    flr_area = sum(floor_areas)
    
    # add in roof and floor to facade area
    top_pt = func(0.5,1.0).origin
    top_radius = top_pt.distance(Point(0,0,top_pt.z))
    fac_area += math.pi * top_radius**2
    
    return (fac_area, flr_area)
    
    #TODO: adde floor count


def surf_to_mesh(func):
    msh = Mesh()
    for v in v_vals:
        for u in u_vals:
            msh.append(func(u,v))
    
    res_u = len(u_vals)
    # simple triangulation style
    for v in range(len(v_vals)):
        row = v*res_u
        for u in range(len(u_vals)-1):
            pi_0 = row+u
            pi_1 = row+u+1
            pi_2 = row+u+res_u+1
            pi_3 = row+u+res_u
            msh.add_face(pi_0,pi_1,pi_2,pi_3)
        if do_close:
            #last two faces in the row
            pi_0 = row+res_u-1
            pi_1 = row+0
            pi_2 = row+res_u
            pi_3 = row+res_u-1+res_u
            msh.add_face(pi_0,pi_1,pi_2,pi_3)
    
    return msh

a.put(surf_to_mesh)
b.put(facade_and_floor_area)
