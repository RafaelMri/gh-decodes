                    <item name="CodeInput" type_name="gh_string" type_code="10">
best_fit = thresh
pts = []
for ri in Interval().divide(res,include_last=True):
    for gi in Interval().divide(res,include_last=True):
        if gi == 0: gi = 0.001
        for bi in Interval().divide(res,include_last=True):
            surf = make_surf(ri,gi,bi)
            fit = fitness(surf)
            fit = fit[0]/fit[1]
            clr = Color(ri,gi,bi)
            if fit &lt;= best_fit:
                best_fit = fit
                best_msh = make_msh(surf)
                best_msh.set_color(clr)
                xf = Xform.translation(Vec(ri,gi,bi)-Vec())
                pt = Point(ri,gi,bi)
                new_pt = pt * xf
                pts.append(new_pt)
                
                best_msh = msh *xf 
a.put(pts)
b.put(best_msh)
c.put(best_fit)
