                    <item name="CodeInput" type_name="gh_string" type_code="10">import math


def rot_surface(rad_cap, rad_mid, mid_height):
    pts = [Point(rad_cap+rad_cap,rad_mid,mid_height),Point(rad_mid+rad_cap,rad_mid,mid_height+mid_height),Point(rad_cap+rad_cap,rad_mid,mid_height+1)]
    def func(u,v):        
        angle = Interval.twopi().eval(u)
        xf = Xform.rotation(axis=Vec(rad_cap,rad_mid,mid_height+1)- Vec(rad_cap,rad_mid,mid_height),angle=angle)
        rot_pts = [pt*xf for pt in pts]
        
        crv = Curve.bezier(rot_pts)
        
        return crv.eval(v)
        
    return func

a.put(rot_surface)</item>
