def rot_surface(rad_cap, rad_mid, mid_height):
    pts = [Point(rad_cap,0,0),Point(rad_mid,0,mid_height),Point(rad_cap,0,1)]
    def func(u,v):        
        angle = Interval.twopi().eval(u)
        xf = Xform.rotation(axis=Vec(0,0,1),angle=angle)
        rot_pts = [pt*xf for pt in pts]
        
        crv = Curve.bezier(rot_pts)
        
        return crv.eval(v)
        
    return func

a.put(rot_surface)</item>
