                    <item name="NickName" type_name="gh_string" type_code="10">Decodes</item>
                    <item name="Description" type_name="gh_string" type_code="10">A python scriptable component that implements the Decodes library</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">dbb771fc-ebae-4d5c-8fea-03df7005a76c</item>
                    <item name="Locked" type_name="gh_bool" type_code="1">true</item>
                    <item name="HideInput" type_name="gh_bool" type_code="1">true</item>
                    <item name="CodeInput" type_name="gh_string" type_code="10">#IN:   a function f(u,v) describing a parametric surface on the domain [0,2*pi]x[0,1]
#OUT:  Surface expressed as a mesh, simple triangulation style

u0 = domain[0]
u1 = domain[1]
v0 = domain[2]
v1 = domain[3]

count_u = int(res_u)
count_v = int(res_v)
tol_u = (u1-u0)/res_u
tol_v = (v1-v0)/res_v
u_vals = [u0 + i*tol_u for i in range(count_u+1)]
v_vals = [v0 + i*tol_v for i in range(count_v+1)]


msh = Mesh()
for v in v_vals:
    for u in u_vals:
        msh.append(func(u,v))

for j in range(count_v):
    for i in range(count_u):
        index = j*(count_u + 1) + i
        msh.add_face(index,index + 1, index + count_u + 2)
        msh.add_face(index, index + count_u + 2, index + count_u + 1)

a.put(msh)</item>
