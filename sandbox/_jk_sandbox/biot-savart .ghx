                    <item name="CodeInput" type_name="gh_string" type_code="10">#Function resulting from the product of lines
w = 0.5*rec_w
h = 0.5*rec_h

def func(u,v):
    x = u
    y = v
    z = 0.4*(u + 0.5*rec_w)*(u - 0.5*rec_w)*(v + 0.5*rec_h)*(v - 0.5*rec_h)
    return Point(x,y,z)
     
srf = Surface(func, Interval(-w,w), Interval(-h,h), tol_u = 0.08, tol_v = 0.08)
a.put(srf.surrogate)</item>
