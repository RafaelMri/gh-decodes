                <item name="GUID" type_name="gh_guid" type_code="9">fccad19e-dcb6-44ab-8edb-54dd6ab7e966</item>
                <item name="Name" type_name="gh_string" type_code="10">Decodes Python Script</item>
                <item name="Lib" type_name="gh_guid" type_code="9">30f802e3-1f17-2416-d927-d82433eca0a2</item>
              </items>
              <chunks count="1">
                <chunk name="Container">
                  <items count="8">
                    <item name="props_visible" type_name="gh_bool" type_code="1">false</item>
                    <item name="Name" type_name="gh_string" type_code="10">Decodes Python Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">Decodes</item>
                    <item name="Description" type_name="gh_string" type_code="10">A python scriptable component that implements the Decodes library</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">45916949-575a-4e05-89c8-14e9a1381ccf</item>
                    <item name="HideInput" type_name="gh_bool" type_code="1">true</item>
                    <item name="CodeInput" type_name="gh_string" type_code="10">"""
Ouputs a nontrimmed biot-savart surface given a polygonal boundary inside the 
rectangular bounding box
"""


zero = 10e-12
w = 0.5*rec_w
h = 0.5*rec_h
rec = PLine([Point(w,h), Point(-w,h), Point(-w,-h), Point(w,-h), Point(w,h)])

#ex of a convex polygon 
#poly = PLine([Point(w,0), Point(0.5*w,h), Point(0,h), Point(-w,0), Point(-0.5*w,-h), Point(0, -h), Point(w, 0)])

#ex of a nonconvex polygon
poly = PLine([Point(0.5*w,0), Point(w, h), Point(-w, 0.5*h), Point(w, -h), Point(0.5*w, 0)])

a.put(poly)


def magnetic_field(pt_in, seg):
    vec_a = Vec(seg.spt - pt_in)
    vec_b = Vec(seg.ept - pt_in)
    mag_a = vec_a.length
    mag_b = vec_b.length
    a_cross_b = vec_a.x*vec_b.y - vec_a.y*vec_b.x
    bottom = mag_a*mag_b*(mag_a*mag_b + vec_a.dot(vec_b))
    if (bottom &gt; zero):
        return a_cross_b*(mag_a + mag_b)/bottom
    else: return zero
   

def func(u,v):
    x = u
    y = v
    if (abs(abs(x) - w) &gt; zero) and (abs(abs(y) - h) &gt; zero):
        this_magnetic_field = 0
        for i in range(len(poly.pts)-1):
            seg = poly.edges[i]
            this_magnetic_field  += magnetic_field(Point(x,y), seg)
        if this_magnetic_field &gt; zero:      
            z = 2.0/this_magnetic_field
        else: z = 0
    else: z = 0
    return Point(x,y,z)
     
     
srf = Surface(func, Interval(-w,w), Interval(-h, h), tol_u = 0.08, tol_v = 0.08)
a.put(srf.surrogate)
