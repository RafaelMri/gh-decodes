                    <item name="CodeInput" type_name="gh_string" type_code="10">zero = 10e-12
w = 0.5*rec_w
h = 0.5*rec_h
rec = PLine([Point(w,h), Point(-w,h), Point(-w,-h), Point(w,-h), Point(w,h)])

def magnetic_field(pt_in, seg):
    vec_a = Vec(seg.spt - pt_in)
    vec_b = Vec(seg.ept - pt_in)
    mag_a = vec_a.length
    mag_b = vec_b.length
    a_cross_b = vec_a.x*vec_b.y - vec_a.y*vec_b.x
    bottom = mag_a*mag_b*(mag_a*mag_b + vec_a.dot(vec_b))
    if (bottom &gt; zero):
        return a_cross_b*(mag_a + mag_b)/bottom
    else: return zero
   

def func(u,v):
    x = u
    y = v
    if (abs(abs(x) - w) &gt; zero) and (abs(abs(y) - h) &gt; zero):
        this_magnetic_field = 0
        for i in range(len(rec.pts)-1):
            seg = rec.edges[i]
            this_magnetic_field  += magnetic_field(Point(x,y), seg)
        this_magnetic_field /= 2.0
        z = 1.0/this_magnetic_field
    else: z = 0
    return Point(x,y,z)
     
     
srf = Surface(func, Interval(-w,w), Interval(-h, h), tol_u = 0.08, tol_v = 0.08)
a.put(srf.surrogate)
