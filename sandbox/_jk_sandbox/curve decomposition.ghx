                    <item name="CodeInput" type_name="gh_string" type_code="10">import math

def leastSquaresLineTotal(pts_in):
    x_m = 0
    y_m = 0
    xsq_m = 0
    ysq_m = 0
    xy_m = 0
    
    for pt in pts_in :
        x_m = x_m + pt.x 
        y_m = y_m + pt.y
        xsq_m = xsq_m + pt.x*pt.x
        ysq_m = ysq_m + pt.y*pt.y
        xy_m = xy_m + pt.x*pt.y
    
    N = len(pts_in)
    x_m = x_m/N
    y_m = y_m/N
    xsq_m = xsq_m/N
    ysq_m = ysq_m/N
    xy_m = xy_m/N
    
    if (xy_m != x_m*y_m):
        B = 0.5*((ysq_m - y_m*y_m)-(xsq_m - x_m*x_m))/(x_m*y_m -xy_m)
        m = -B + math.sqrt(B*B + 1)
        y_coeff = y_m - m*x_m
        p_ls = Point(0, y_coeff)
        u_ls = Vec(1, m).normalized()
        
    d_min = 0
    d_max = 0
    for pt in pts_in:
        d = Vec(pt - p_ls).dot(u_ls)
        if (d &gt; d_max): d_max = d
        elif (d &lt; d_min): d_min = d
        
    p_start = p_ls + u_ls*d_min
    p_end = p_ls + u_ls*d_max
    
    return Segment(p_start, p_end)
    


pts = []

n = 50
for i in range(n):
    pts.append(Point(10*math.cos(1.4*math.pi*i/n), 4*math.sin(1.4*math.pi*i/n)))

a.put(pts)  
b.put(leastSquaresLineTotal(pts))</item>
