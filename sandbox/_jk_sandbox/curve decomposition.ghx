                    <item name="CodeInput" type_name="gh_string" type_code="10">def furthestFromLeastSquaresLine(pts_in):
    x_m = 0
    y_m = 0
    xsq_m = 0
    xy_m = 0
    
    for pt in pts_in :
        x_m = x_m + pt.x 
        y_m = y_m + pt.y
        xsq_m = xsq_m + pt.x*pt.x
        xy_m = xy_m + pt.x*pt.y
    
    n = len(pts_in)
    x_m = x_m/n
    y_m = y_m/n
    xsq_m = xsq_m/n
    xy_m = xy_m/n
    
    if (xsq_m != x_m*x_m):
        m = (xy_m - x_m*y_m)/(xsq_m - x_m*x_m)
        y_coeff = y_m - m*x_m
        p_ls = Point(0, y_coeff)
        n_ls = Vec(-m, 1).normalized()
    else:
        p_ls = Point(x_m, 0)
        n_ls = Vec(1,0)
        
    d_max = 0
    i_max = 0
    for i in range(n):
        d = abs(Vec(pts_in[i] - p_ls).dot(n_ls))
        if (d &gt; d_max):
            d_max = d
            i_max = i
    return [i_max, d_max]


def furthestFromLeastSquaresLineTotal(pts_in):
    x_m = 0
