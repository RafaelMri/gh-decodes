                    <item name="CodeInput" type_name="gh_string" type_code="10">def furthestFromLeastSquaresLineTotal(pts_in):
    x_m = 0
    y_m = 0
    xsq_m = 0
    ysq_m = 0
    xy_m = 0
    
    for pt in pts_in :
        x_m = x_m + pt.x 
        y_m = y_m + pt.y
        xsq_m = xsq_m + pt.x*pt.x
        ysq_m = ysq_m + pt.y*pt.y
        xy_m = xy_m + pt.x*pt.y
    
    n = len(pts_in)
    x_m = x_m/n
    y_m = y_m/n
    xsq_m = xsq_m/n
    ysq_m = ysq_m/n
    xy_m = xy_m/n
    
    if (xy_m != x_m*y_m):
        B = 0.5*((ysq_m - y_m*y_m)-(xsq_m - x_m*x_m))/(x_m*y_m -xy_m)
        m = [-B + math.sqrt(B*B + 1), -B - math.sqrt(B*B + 1)]
        y_coeff = [y_m - m[0]*x_m, y_m - m[1]*x_m]
        p = [Point(0, y_coeff[0]), Point(0, y_coeff[1])]
        n0 = Vec(-m[0], 1).normalized()
        n1 = Vec(-m[1], 1).normalized()
        #test for the line that gives the least (not the most!) distance
        d0 = 0
        d1 = 0
        for pt in pts_in:
            d0 = d0 +  abs(Vec(pt - p[0]).dot(n0))
            d1 = d1 +  abs(Vec(pt - p[1]).dot(n1))
        if d0 &lt; d1:
            p_ls  = p[0]
            n_ls = n0
        else:
            p_ls = p[1]
            n_ls = n1
    else:
        p_ls = Point(x_m, 0)
        n_ls = Vec(1,0)
        
    d_max = 0
    i_max = 0
    for i in range(n):
        d = abs(Vec(pts_in[i] - p_ls).dot(n_ls))
        if (d &gt; d_max):
            d_max = d
            i_max = i
    #print d_max, p_ls, n_ls
    print [i_max, d_max]
    return [i_max, d_max]

      
def decomposeCurve(pts_in, i_left, indices):
    [i_star, dist] = furthestFromLeastSquaresLineTotal(pts_in)
    indices.append(i_left + i_star)
    if (dist &gt; threshold):
        if (i_star != 0):
            decomposeCurve(pts_in[:i_star], i_left, indices)
        if (i_star != len(pts_in)-1):
            decomposeCurve(pts_in[i_star+1:], i_left+i_star+1, indices)


curve_in = Curve.bezier(pts)
a.put(curve_in)

pts_curve = []
n = 50
pts_curve = [curve_in.eval(i/n).origin for i in range(n+1)]

i_keep = []
decomposeCurve(pts_curve, 0, i_keep)
print len(i_keep)," indices to keep:", sorted(i_keep)
pts_keep = [curve_in.eval(i/n).origin for i in sorted(i_keep)]
b.put(PLine(pts_keep))
