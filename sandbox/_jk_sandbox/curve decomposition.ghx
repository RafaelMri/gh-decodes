                    <item name="CodeInput" type_name="gh_string" type_code="10">class Circle():
    
    # TODO: this class currently assumes a circle on the xy axis
    
    def __init__(self,plane,radius):
        self.pln = plane
        self.rad = radius
        
    @property
    def cpt(self):
        return self.pln.origin
        
    def intersections(self,other):
        # TODO: this func currently assumes a circle on the xy axis
        if not self.pln.is_coplanar( other.pln ) : 
            print self.pln.near(other.pln.origin)[0].distance(other.pln.origin)
            print 'circles not coplanar'
            return False
        d = self.cpt.distance(other.cpt)
        if d == 0 : 
            print 'circles share a center point'
            return False
        a = (self.rad**2 - other.rad**2 + d**2)/(2*d)
        h2 = self.rad**2 - a**2
        if h2 &lt; 0 : 
            print 'what, huh?'
            return False
        h = math.sqrt(h2)
        pt = ( other.cpt - self.cpt ) * (a/d) + self.cpt
        if h == 0 : return pt
        vec = Vec(self.cpt,pt).cross(self.pln.vec).normalized(h)
        return [pt - vec, pt + vec]
        
    def to_gh(self):
        import Rhino.Geometry as rg
        plane = rg.Plane(rg.Point3d(self.pln.origin.x,self.pln.origin.y,self.pln.origin.z),rg.Vector3d(self.pln.vec.x,self.pln.vec.y,self.pln.vec.z))
        return rg.Circle(plane,self.rad)
        
a.put(Circle)</item>
