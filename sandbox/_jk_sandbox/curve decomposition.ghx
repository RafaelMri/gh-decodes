                    <item name="CodeInput" type_name="gh_string" type_code="10">#Returns a best fit circle using the modified least squares method
#TODO - implement exceptions eg. A*C == B*B 
#     - implement Arc as a subclass of Circle

def leastSquaresCircle(pts_in):
    x = 0
    y = 0
    x = 0
    xsq = 0
    ysq = 0
    xy = 0
    xysq = 0
    xsqy = 0
    xcube = 0
    ycube = 0
    
    for pt in pts_in :
        x = x + pt.x 
        y = y + pt.y
        xsq = xsq + pt.x*pt.x
        ysq = ysq + pt.y*pt.y
        xy = xy + pt.x*pt.y
        xysq = xysq + pt.x*pt.y*pt.y
        xsqy = xsqy + pt.x*pt.x*pt.y
        xcube = xcube + pt.x*pt.x*pt.x
        ycube = ycube + pt.y*pt.y*pt.y
       
    n = len(pts_in)

    A = n*xsq - x*x
    B = n*xy - x*y
    C = n*ysq - y*y
    D = 0.5*(n*xysq - x*ysq + n*xcube - x*xsq)
    E = 0.5*(n*xsqy - y*xsq + n*ycube - y*ysq)
    
    denom = A*C - B*B
    if (denom != 0):
        center = Point((D*C - B*E)/denom, (A*E - B*D)/denom)
    else: print "yikes"     
    rad = 0
    for pt in pts_in:
        rad = rad + pt._distance(center)        
    rad = rad/n
    circ_out = Circle(Plane(center, Vec(0,0,1)), rad)
    return circ_out.to_gh()
    
    #TODO - implement Arc as subclass of Circle and return arc
    #angle_start = 2*math.pi
