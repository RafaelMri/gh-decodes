def leastSquaresLine(pts_in):
    x_m = 0
    y_m = 0
    xsq_m = 0
    xy_m = 0
    
    for pt in pts_in :
        x_m = x_m + pt.x 
        y_m = y_m + pt.y
        xsq_m = xsq_m + pt.x*pt.x
        xy_m = xy_m + pt.x*pt.y
    
    N = len(pts_in)
    x_m = x_m/N
    y_m = y_m/N
    xsq_m = xsq_m/N
    xy_m = xy_m/N
    
    if (xsq_m != x_m*x_m):
        m = (xy_m - x_m*y_m)/(xsq_m - x_m*x_m)
        y_coeff = y_m - m*x_m
        p_ls = Point(0, y_coeff)
        u_ls = Vec(1, m).normalized()
        
    d_min = 0
    d_max = 0
    for pt in pts_in:
        d = Vec(pt - p_ls).dot(u_ls)
        if (d &gt; d_max): d_max = d
        elif (d &lt; d_min): d_min = d
        
    p_start = p_ls + u_ls*d_min
    p_end = p_ls + u_ls*d_max
    
    return Segment(p_start, p_end)
    

import math

pts = []

n = 50
for i in range(n):
    pts.append(Point(10*math.cos(1.4*math.pi*i/n), 4*math.sin(1.4*math.pi*i/n)))

a.put(pts)  
b.put(leastSquaresLine(pts))</item>
