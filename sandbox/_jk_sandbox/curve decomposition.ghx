                    <item name="CodeInput" type_name="gh_string" type_code="10">#Returns a best fit line defined by ordinary least squares
#TODO - implement exception eg. xsq_m == x_m*x*m


def leastSquaresLine(pts_in):
    x_m = 0
    y_m = 0
    xsq_m = 0
    xy_m = 0
    
    for pt in pts_in :
        x_m = x_m + pt.x 
        y_m = y_m + pt.y
        xsq_m = xsq_m + pt.x*pt.x
        xy_m = xy_m + pt.x*pt.y
    
    n = len(pts_in)
    x_m = x_m/n
    y_m = y_m/n
    xsq_m = xsq_m/n
    xy_m = xy_m/n
    
    if (xsq_m != x_m*x_m):
        m = (xy_m - x_m*y_m)/(xsq_m - x_m*x_m)
        y_coeff = y_m - m*x_m
        p_ls = Point(0, y_coeff)
        u_ls = Vec(1, m).normalized()
    else: print "yikes"
        
    d_min = Vec(pts_in[0]-p_ls).dot(u_ls)
    d_max = d_min
    for pt in pts_in:
        d = Vec(pt - p_ls).dot(u_ls)
        if (d &gt; d_max): d_max = d
        elif (d &lt; d_min): d_min = d
        
    p_start = p_ls + u_ls*d_min
    p_end = p_ls + u_ls*d_max
    
    return Segment(p_start, p_end)

a.put(leastSquaresLine)</item>
