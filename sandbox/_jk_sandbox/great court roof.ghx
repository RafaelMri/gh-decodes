                    <item name="CodeInput" type_name="gh_string" type_code="10">import math

def func(t):
    c = math.cos(t)
    s = math.sin(t)
    factor1 = 0.1
    factor2 = 0.5
    r = rad*(1 + factor1*math.cos(2*t) + factor2*math.cos(3*t))
    pt_out = Point(r*c, r*s)
    return pt_out
     
