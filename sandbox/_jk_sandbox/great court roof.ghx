                    <item name="CodeInput" type_name="gh_string" type_code="10">

# convert res_u to nearest multiple of 4.0
if res_u%4 != 0 : 
    res_u = res_u + (4 - res_u%4)
    print "res_u set to ", res_u

msh = Mesh()
for v in range(res_v+1):
    for u in range(res_u):
        msh.append(func(u/res_u,v/res_v))

# simple triangulation style
for v in range(res_v):
    row = v*res_u
    for u in range(res_u-1):
        pi_0 = row+u
        pi_1 = row+u+1
        pi_2 = row+u+res_u
        pi_3 = row+u+res_u+1
        msh.add_face(pi_0,pi_1,pi_2)
        msh.add_face(pi_1,pi_3,pi_2)
    #last two faces in the row
    pi_0 = row+res_u-1
    pi_1 = row+0
    pi_2 = row+res_u-1+res_u
    pi_3 = row+res_u
    msh.add_face(pi_0,pi_1,pi_2)
    msh.add_face(pi_1,pi_3,pi_2)

a.put(msh)
