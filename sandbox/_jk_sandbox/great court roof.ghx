                    <item name="CodeInput" type_name="gh_string" type_code="10">

# convert res_u to nearest multiple of 4.0
if res_u%4 != 0 : 
    res_u = res_u + (4 - res_u%4)
    print "res_u set to ", res_u

msh = Mesh()
for v in range(res_v+1):
    for u in range(res_u):
        msh.append(func(u/res_u,v/res_v))

# centroid triangulation style
for v in range(res_v):
    row = v*res_u
    for u in range(res_u-1):
        pi = [row+u,row+u+1,row+u+res_u,row+u+res_u+1]
        msh.append(Point.centroid([msh.pts[i] for i in pi])) # add one more vertex at the center
        pi.append(len(msh.pts)-1)
        msh.add_face(pi[0],pi[1],pi[4])
        msh.add_face(pi[1],pi[3],pi[4])
        msh.add_face(pi[3],pi[2],pi[4])
        msh.add_face(pi[2],pi[0],pi[4])
    #last two faces in the row
    pi = [ row+res_u-1, row+0, row+res_u-1+res_u, row+res_u ]
    msh.append(Point.centroid([msh.pts[i] for i in pi])) # add one more vertex at the center
    pi.append(len(msh.pts)-1)
    msh.add_face(pi[0],pi[1],pi[4])
    msh.add_face(pi[1],pi[3],pi[4])
    msh.add_face(pi[3],pi[2],pi[4])
    msh.add_face(pi[2],pi[0],pi[4])

a.put(msh)
