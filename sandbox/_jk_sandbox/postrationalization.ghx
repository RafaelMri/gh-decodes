def deval_normal(u,v):
    pt_uv = surf.deval(u,v)
    pt, u_pos, u_neg, v_pos, v_neg = surf._nudged(u,v,True)
    ngbr = Mesh()
    ngbr.append(pt_uv)
    ngbr.append(pt_uv +  u_pos )
    ngbr.append(pt_uv +  u_pos  +  v_pos)
    ngbr.append(pt_uv +  v_pos )
    ngbr.append(pt_uv +  u_neg  +  v_pos)
    ngbr.append(pt_uv +  u_neg )
    ngbr.append(pt_uv +  u_neg  +  v_neg)
    ngbr.append(pt_uv +  v_neg )
    ngbr.append(pt_uv +  u_pos  +  v_neg)
    

    #Triangular mesh
    ngbr.add_face(0,1,2)
    ngbr.add_face(0,2,3)
    ngbr.add_face(0,3,5)
    ngbr.add_face(0,5,6)
    ngbr.add_face(0,6,7)
    ngbr.add_face(0,7,1)

    N_vec = Vec.average([ngbr.face_normal(k) for k in range(len(ngbr.faces))])
    if test:
        N_vec_test = Vec(0,0,0)
        for k in range(len(ngbr.faces)):
            verts = ngbr.face_pts(k)
            face_area = 0.5*Vec(verts[0],verts[1]).cross(Vec(verts[0],verts[2])).length
            N_vec_test = N_vec_test + ngbr.face_normal(k)*face_area 
        print "N_vec comparision:", N_vec.normalized(), N_vec_test.normalized()
    return N_vec.normalized()
   
 

def deviation_from_plane(vecs):
    avg_v = Vec.average(vecs)
    dev = []
    for v in vecs:
        dev.append((v - v.projected(avg_v)).length)
    return max(dev)
        
    
count_u = int(res_u)
count_v = int(res_v)
msh = surf.to_mesh(divs_u=count_u,divs_v=count_v)

u_val = Interval(surf.u0, surf.u1).divide(count_u,True)
v_val = Interval(surf.v0, surf.v1).divide(count_v,True)

# Evaluate the normals at each vertex of the mesh
normals = []
for v in v_val:
   for u in u_val:
        N_vec = deval_normal(u,v)
        normals.append(N_vec)
        if test: test_out.put(Segment(surf.deval(u,v), N_vec*0.25))

dev_plane = []
for k in range(len(msh.faces)):
    vert_normals = [normals[i] for i in msh.faces[k]]
    if test and ((k) % count_u == 0):
        verts = msh.face_pts(k)
        test_out.put(verts[0])
        avg_v = Vec.average(vert_normals)
        print vert_normals[1], vert_normals[2]
        test_out.put(Segment(msh.face_centroid(k), avg_v.normalized()*0.25))
    dev_plane.append(deviation_from_plane(vert_normals))

color_near = Color.RGB(1, 1, 1)
color_far =  Color.RGB(1, 0, 0)
msh_faces = Mesh.explode(msh)

print "Min deviation:", min(dev_plane)
print "Max deviation:", max(dev_plane)
if max(dev_plane) &gt; 1:
    dev_plane =  [Interval.remap(i, Interval(0, max(dev_plane))) for i in dev_plane]

for i in range(len(msh_faces)):
    face = msh_faces[i]
    face.set_color(Color.interpolate(color_near,color_far,dev_plane[i]))
    a.put(face)</item>
