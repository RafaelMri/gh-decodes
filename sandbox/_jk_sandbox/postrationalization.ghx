                    <item name="props_visible" type_name="gh_bool" type_code="1">false</item>
                    <item name="Name" type_name="gh_string" type_code="10">Decodes Python Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">Decodes</item>
                    <item name="Description" type_name="gh_string" type_code="10">A python scriptable component that implements the Decodes library</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">c5731172-cc8e-444d-b9fb-b3ae0e1e57a4</item>
                    <item name="Hidden" type_name="gh_bool" type_code="1">true</item>
                    <item name="Locked" type_name="gh_bool" type_code="1">true</item>
                    <item name="HideInput" type_name="gh_bool" type_code="1">true</item>
                    <item name="CodeInput" type_name="gh_string" type_code="10"># Parametrized on the domain [0,1]x[0,1].  

def func_crv(t):
    return Point(t, 0, t*t*t/3.0 + t*t)

def func_rail(t):
    return Point(0, t, -1.5*t*t)

crv = Curve(func_crv,Interval(-2,1.5))
rail = Curve(func_rail, Interval(-1,1))
origin = Point(0,0,0)

def func(u,v):
    u_value = rail.domain.length*u + rail.domain.a
    v_value = crv.domain.length*v + crv.domain.a
    vec = origin - rail.deval(u_value)
    return crv.deval(v_value) - vec
   
surf = Surface(func)
a.put(surf)
b.put(surf.surrogate)</item>
