                    <item name="CodeInput" type_name="gh_string" type_code="10">"""
IN:  square region given in domain space by rgn_ctr (center of a normalized domain) 
and rgn_width (normalized width of box) 
OUT: 
"""

import math

def deval_normal(u,v):
    pt_uv = surf.deval(u,v)
    pt, u_pos, u_neg, v_pos, v_neg = surf._nudged(u,v,True)
    ngbr = Mesh()
    ngbr.append(pt_uv)
    ngbr.append(pt_uv +  u_pos )
    ngbr.append(pt_uv +  u_pos  +  v_pos)
    ngbr.append(pt_uv +  v_pos )
    ngbr.append(pt_uv +  u_neg  +  v_pos)
    ngbr.append(pt_uv +  u_neg )
    ngbr.append(pt_uv +  u_neg  +  v_neg)
    ngbr.append(pt_uv +  v_neg )
    ngbr.append(pt_uv +  u_pos  +  v_neg)
    

    #Triangular mesh
    ngbr.add_face(0,1,2)
    ngbr.add_face(0,2,3)
    ngbr.add_face(0,3,5)
    ngbr.add_face(0,5,6)
    ngbr.add_face(0,6,7)
    ngbr.add_face(0,7,1)

    N_vec = Vec.average([ngbr.face_normal(k) for k in range(len(ngbr.faces))])
    if test:
        N_vec_test = Vec(0,0,0)
        for k in range(len(ngbr.faces)):
            verts = ngbr.face_pts(k)
            face_area = 0.5*Vec(verts[0],verts[1]).cross(Vec(verts[0],verts[2])).length
            N_vec_test = N_vec_test + ngbr.face_normal(k)*face_area 
        print "N_vec comparision:", N_vec.normalized(), N_vec_test.normalized()

    return N_vec.normalized()
   
 

def deviation_from_plane(vecs):
    avg_v = Vec.average(vecs)
    dev = []
    for v in vecs:
        dev.append((v - v.projected(avg_v)).length)
    return max(dev)
        
    
count_u = int(res_u)
count_v = int(res_v)
srf_msh = surf.to_mesh(divs_u=count_u,divs_v=count_v)
a.put(srf_msh)

u0 = surf.u0
u1 = surf.u1
v0 = surf.v0
v1 = surf.v1

u0_regn = max(u0, u0 + (u1-u0)*(math.floor((rgn_ctr.x - rgn_width*0.5)*count_u)/count_u))
u1_regn = min(u1, u0 + (u1-u0)*(math.ceil((rgn_ctr.x + rgn_width*0.5)*count_u)/count_u))
v0_regn = max(v0, v0 + (v1-v0)*(math.floor((rgn_ctr.y - rgn_width*0.5)*count_v)/count_v))
v1_regn = min(v1, v0 + (v1-v0)*(math.ceil((rgn_ctr.y + rgn_width*0.5)*count_v)/count_v))
if test: test_out.put([surf.deval(u0_regn, v0_regn), surf.deval(u1_regn, v0_regn), surf.deval(u1_regn, v1_regn), surf.deval(u0_regn, v1_regn)])

div_u = (u1-u0)/count_u
div_v = (v1-v0)/count_v
count_u_regn = int(math.ceil((u1_regn - u0_regn)/div_u))
count_v_regn = int(math.ceil((v1_regn - v0_regn)/div_v))
u_val = Interval(u0_regn, u1_regn).divide(count_u_regn,True)
v_val = Interval(v0_regn, v1_regn).divide(count_v_regn,True)


# Evaluate the normals at each vertex in the given region
normals = []
for v in v_val:
   for u in u_val:
        N_vec = deval_normal(u,v)
        normals.append(N_vec)
        if test: test_out.put(Segment(surf.deval(u,v), N_vec*0.25))


dev_plane = deviation_from_plane(normals)
print "Deviation from plane: ", dev_plane

#Color in the faces in the region according to the region's deviation from planar
color_near = Color.RGB(1, 1, 1)
color_far =  Color.RGB(1, 0, 0)

i0 = int((u0_regn-u0)/div_u)
j0 = int((v0_regn-v0)/div_v)
i1 = int((u1_regn-u0)/div_u)
j1 = int((v1_regn-v0)/div_v)

for j in range(j0, j1):
    for i in range(i0, i1):
        pts = srf_msh.face_pts(j*count_u + i)
        nface = [0,1,2,3]
        face_msh = Mesh(pts,[nface])
        face_msh.set_color(Color.interpolate(color_near,color_far,dev_plane))
        a.put(face_msh)</item>
