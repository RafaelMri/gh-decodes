                    <item name="CodeInput" type_name="gh_string" type_code="10">import math

def func(u,v): return Point(u,v,math.sin(u*v)) # sine surface
surf_a = Surface(func, Interval(0, 1), Interval(-1,1))
a.put(surf_a)

def func(u,v): return Point(u,v,(u**3-3*u*v**2)/4) # monkey saddle
surf_b = Surface(func, Interval(-1, 1), Interval(-1,1))
b.put(surf_b)

def func(u,v): return Point(u,v,((1/3)*u**3+u*v**2+2*(u**2-v**2))/4 ) # handkerchief surface
surf_c = Surface(func, Interval(-1, 1), Interval(-1,1))
c.put(surf_c)

def func(u,v): return Point(u*v,u,v**2) #umbrella surface
surf_d = Surface(func, Interval(0.01, 1), Interval(-1,1))
d.put(surf_d)

#steiner surface
def func(u,v):
    x = 0.5*math.cos(v)*math.cos(v)*math.sin(2*u)
    y = 0.5*math.sin(u)*math.sin(2*v)
    z = 0.5*math.cos(u)*math.sin(2*v)
    return Point(x,y,z)
surf_e = Surface(func, Interval(0.01, math.pi), Interval(0.01, math.pi))
e.put(surf_e)</item>
