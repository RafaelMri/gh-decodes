#IN:  a function defining a translational surface parametrized on the domain [0,1]x[0,1]

def func_u(t):
    return surf._func(0,t)

def func_v(t):
    return surf._func(t,0)
 
def calc_curvature(func_crv,t):
    #nearest neighbors of point in question; used for discrete approximations
    #calculations;
    res_curvature = grid_res
    pt_out = func_crv(t)
    if (t == 0):
        pt_t = func_crv(0.1*res_curvature)
        pt_plus = func_crv(0.2*res_curvature)
        pt_minus = func_crv(0)    
    elif (t &gt; 0) and (t &lt; 0.5*res_curvature):
        pt_t = pt_out
        pt_plus = func_crv(2*t)
        pt_minus = func_crv(0)
    elif (t &gt; 1-0.5*res_curvature) and (t &lt; 1):
        pt_t = pt_out
        pt_plus = func_crv(1)
        pt_minus = func_crv(0.5*t)
    elif (t == 1):
        pt_t = func_crv(1-0.1*res_curvature)
        pt_plus = func_crv(1)
        pt_minus = func_crv(1-0.2*res_curvature)
    else:
        pt_t = pt_out
        pt_plus = func_crv(t + 0.5*res_curvature)
        pt_minus = func_crv(t - 0.5*res_curvature)

    #calculates the curvature at t for given grid_resolution
    v1 = Vec(pt_plus - pt_t)
    v2 = Vec(pt_minus - pt_t)
    v3 = Vec(pt_plus - pt_minus)
    try:
        rad_osc = 0.5*v1.length*v2.length*v3.length/(v1*v3).length
        return 1.0/rad_osc
    except:
        print "curvature calculation error!"
        print t
        print v1, v2, v3
        return False

def decompose_surface(u0=0,u1=1,v0=0,v1=1):
    i0 = int(grid_count*u0)
    i1 = int(grid_count*u1)
    j0 = int(grid_count*v0)
    j1 = int(grid_count*v1)
    if (i1 &gt; i0+1) and (j1 &gt; j0+1):
        max_curvature = max(max(curvature_u[i0: i1+1]), max(curvature_v[j0:j1+1]))
        #print "max curvature", max_curvature
        if (max_curvature &gt; threshold):
            u_mid = (u0 + u1)/2
            v_mid = (v0 + v1)/2
            decomposeSurface(u0, u_mid, v0, v_mid)
            decomposeSurface(u_mid, u1, v0, v_mid)
            decomposeSurface(u0, u_mid, v_mid, v1)
            decomposeSurface(u_mid, u1, v_mid, v1)
        else:
            index0= j0*(grid_count+1)+i0
            index1= j0*(grid_count+1)+i1
            index2= j1*(grid_count+1)+i1
            index3= j1*(grid_count+1)+i0
            msh.add_face(index0, index1, index2, index3)
    else:
        index0= j0*(grid_count+1)+i0
        index1= j0*(grid_count+1)+i1
        index2= j1*(grid_count+1)+i1
        index3= j1*(grid_count+1)+i0
        msh.add_face(index0, index1, index2, index3)
    
    return msh

            
grid_count = 32
grid_res = 1.0/grid_count

curvature_u = [calc_curvature(func_u, i*grid_res) for i in range(grid_count+1)]
curvature_v = [calc_curvature(func_v, j*grid_res) for j in range(grid_count+1)] 

msh = Mesh()
for i in range(grid_count+1):
    for j in range(grid_count+1):
        msh.append(surf.eval(i*grid_res,j*grid_res))

decompose_surface()
a.put(msh)
b.put(curvature_u)
c.put(curvature_u)</item>
