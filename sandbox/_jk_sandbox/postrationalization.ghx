                    <item name="CodeInput" type_name="gh_string" type_code="10"># A wavy translational surface

import math

def func_crv(t):
    return Point(t, 0, math.sin(t))

def func_rail(t):
    return Point(0, 2*t, -t*t)
    
crv = Curve(func_crv,Interval(0,4.5*math.pi))
rail = Curve(func_rail, Interval(-2,2))
origin = Point(0,0,0)

def func(u,v):
    u_value = rail.domain.length*u + rail.domain.a
    v_value = crv.domain.length*v + crv.domain.a
    vec = origin - rail.deval(u_value)
    return crv.deval(v_value) - vec
   
surf = Surface(func)
a.put(surf)
b.put(surf.surrogate)</item>
