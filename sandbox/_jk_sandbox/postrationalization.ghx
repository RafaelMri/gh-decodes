                    <item name="CodeInput" type_name="gh_string" type_code="10">#A hyperbolic parabolic consructed as a translational surface of the two curves
#(2t, 0, 2*t*t) and (0, 2*t, -2*t*t).  Parametrized on the domain [0,1]x[0,1].  

def func_crv(t):
    return Point(2*t, 0, 2*t*t)

def func_rail(t):
    return Point(0, 2*t, -2*t*t)
   

crv = Curve(func_crv,Interval(-2,2))
rail = Curve(func_rail, Interval(-2,2))
origin = Point(0,0,0)

def func(u,v):
    u_value = rail.domain.length*u + rail.domain.a
    v_value = crv.domain.length*v + crv.domain.a
    vec = origin - rail.deval(u_value)
    return crv.deval(v_value) - vec
   
surf = Surface(func)
a.put(surf)
b.put(surf.surrogate)
</item>
