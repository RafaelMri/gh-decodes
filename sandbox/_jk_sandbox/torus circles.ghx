circles.put(circ1*rot_theta)
circ2 = circle_3pts(func2(0), func2(0.5*pi), func2(pi))
circles.put(circ2*rot_theta)
circ3 = circle_3pts(func3(0), func3(0.5*pi), func3(pi))
circles.put(circ3*rot_theta)
