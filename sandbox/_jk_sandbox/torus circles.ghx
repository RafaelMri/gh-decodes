                    <item name="CodeInput" type_name="gh_string" type_code="10">#IN:   a function f(u,v) describing a parametric surface on the domain [0,1]x[0,1]
#OUT:  Surface expressed as a mesh aligned with isocurves for u, v determined by res_u and res_v 


count_u = int(res_u)
count_v = int(res_v)
tol_u = 1.0/res_u
tol_v = 1.0/res_v
u_vals = [i*tol_u for i in range(count_u+1)]
v_vals = [i*tol_v for i in range(count_v+1)]

#Expressed as a mesh with faces determined by isocurves
msh = Mesh()
for v in v_vals:
    for u in u_vals:
        msh.append(func(u,v))

for j in range(count_v):
    row = j*(count_u+1)
    for i in range(count_u):
        pi_0 = row+i
        pi_1 = row+i+1
        pi_2 = row+i+count_u+2
        pi_3 = row+i+count_u+1
        msh.add_face(pi_0,pi_1,pi_2,pi_3)

a.put(msh)</item>
