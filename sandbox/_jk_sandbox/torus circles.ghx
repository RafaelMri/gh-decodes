                    <item name="CodeInput" type_name="gh_string" type_code="10">"""
Torus 
    x = (R + r*math.cos(v))*math.cos(u)
    y = (R + r*math.cos(v))*math.sin(u)
    z = r*math.sin(v)
    domain = [0, 2*pi]x[0, 2*pi]
"""

import math



def torus_1(u,v):
    u_val = 2*math.pi*u
    v_val = 2*math.pi*v
    rho = R + r*math.cos(v_val)
    pt_out = Point(rho*math.cos(u_val), rho*math.sin(u_val), r*math.sin(v_val))
    return pt_out


#Alternate parametrization that gives isocurves along the Villarceau circles
#[FIX - need to do change of coords]
#v = 'phi'
#u = 'theta'
def torus_2(u,v):
    tilt = r/R
    alpha = math.asin(tilt)
    u_val = 2*math.pi*u
    v_val = 2*math.pi*v
    gamma = v_val
    
    rho = R+math.cos(v_val)*r
    x_ref = rho*math.cos(u_val)
    y_ref = rho*math.sin(u_val)
    z_ref = r*math.cos(v_val)
    
    x_2 = x_ref
    y_2 = y_ref
    z_2 = z_ref
    
    x_1 = r + R*math.cos(gamma)
    y_1 = R*math.sin(gamma)*math.cos(alpha)
    z_1 = r*math.sin(gamma)
    
    psi = math.atan2(x_1*y_2 - x_2*y_1, x_1*x_2 + y_1*y_2)
      
    x_2 = x_1*math.cos(psi) - y_1*math.sin(psi)
    y_2 = x_1*math.sin(psi) + y_1*math.cos(psi)
    z_2 = z_1
    
    pt_out = Point(x_2, y_2, z_2)
    return pt_out
 
 
a.put(torus_1)

b.put(torus_2)</item>
