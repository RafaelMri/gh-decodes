                    <item name="CodeInput" type_name="gh_string" type_code="10">import math

def func(t):
    t_value = 2*math.pi*t
    c = math.cos(t_value)
    s = math.sin(t_value)
    pt_out = Point(r + R*c, math.sqrt(R*R-r*r)*s, r*s)
    return pt_out
     

