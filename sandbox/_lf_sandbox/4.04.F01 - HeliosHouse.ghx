def mesh_pgons(pts_top, pts_mid, pts_btm):
    msh = Mesh()
    msh.append(pts_top)
    msh.append(pts_mid)
    msh.append(pts_btm)
    cnt = len(pgon_top.pts)
    for n in range(cnt-1):
        msh.add_face(n,n+1,n+cnt)
        msh.add_face(n+1,cnt+n+1,cnt+n)
        msh.add_face(cnt+n,2*cnt+n+1,cnt*2+n)
        msh.add_face(cnt+n,cnt+n+1,2*cnt+n+1)
    msh.add_face(cnt-1,0,cnt*2-1)
    msh.add_face(0,cnt,cnt*2-1)
    msh.add_face(cnt*3-1,cnt*2-1,cnt*2)
    msh.add_face(cnt*2,cnt*2-1,cnt)
    return msh

# Decimal range function
def dec_range(start, stop, step):
    r = start
    while r &lt; stop:
        yield r
        r += step

def column(pgon,height,scale):
    # Get the centroid and vertices of the top PGon
    pts_btm = pgon.pts
    # Get the vertices for the mid PGon from a displaced top Pgon. 
    pts_mid = [edge.eval(0.5) + Vec(0,0,max_height*height[0]) for edge in pgon.edges]
    # Get the step size for the domain based on the number of sides of the PGon
    step = math.pi/(len(pgon.edges)-1)
    sin_range = list(dec_range(0, math.pi+.1, step))
    # Create the scale transformation matrix based on a sine function
    xs_list = []
    for i in sin_range:
        xs_list.append(Xform.scale((math.sin(i)*amp)+scale[0], Point.centroid(pts_mid)))
    new_pts_mid = []
    for i, pt in enumerate(pts_mid):
        new_pts_mid.append(pt * xs_list[i])
    
    # Get the vertices for the btm PGon from a displaced top Pgon. 
    pts_top = [pt + Vec(0,0,max_height*height[1]) for pt in pts_btm]
    # Get the step size for the domain based on the number of sides of the PGon
    cos_range = list(dec_range(0, math.pi+.1, step))
    # Create the scale transformation matrix based on a sine function
    xf_list = []
    for i in cos_range:
        xf_list.append(Xform.scale((math.sin(i)*amp)+scale[1], Point.centroid(pts_top)))
    new_pts_top = []
    for i, pt in enumerate(pts_top):
        new_pts_top.append(pt * xf_list[i])
    return mesh_pgons(pts_btm, new_pts_mid, new_pts_top),new_pts_top

