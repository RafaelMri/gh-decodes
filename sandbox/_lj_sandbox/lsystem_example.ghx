                    <item name="CodeInput" type_name="gh_string" type_code="10">class LEngine(object):

    def __init__(self,axiom):
        self.axiom = axiom
        self.rules = []
        self.clear()

    # a function to define rules to be checked
    def add_rule(self,rulething):
        # check on structure of rulething and convert to tuple
        rule = rulething
        try:
            rule.strip()# remove all white spaces from the rule
            # then attempt to split the rule at the arrow
            # and remove any left over white spaces
            rule = [str.strip() for str in rulething.split("-&gt;")]
        except:# if the rule is not in single string format
            try:# check if it is a tuple
                rule = rulething[0],rulething[1]
            except:# and if not, return an error
                raise TypeError("oh snap, that's not a rule")
        # if it is a valid rule, add it to the list of rules
        self.rules.append(rule)

    # a function that replaces a character with its appropriate rule
    def apply_rules(self, char):
        # check all the rules for the one to apply
        for rule in self.rules:
            # if the character matches the first half of the rule tuple
            if char == rule[0]:
                # replace the character with the second half of the rule tuple
                return rule[1]
                break
        # if no rule characters match, return the original character
        return char
    
    # a function that applies character replacement to strings based on rules
    def step(self):
        nxt_gen = ""
        # for every character in the current string
        # check if there is a rule that applies to it and replace
        for chr in self.cur_gen: nxt_gen += str(self.apply_rules(chr))
        self.hist.append(nxt_gen)

    # returns the last generation of applied strings of rules
    @property
    def cur_gen(self): return self.hist[-1]
    
    # removes all rules, and applied strings of rules
    def clear(self):
        self.hist = [self.axiom]
        self.rules = []

