                    <item name="UserText" type_name="gh_string" type_code="10">class LTurtle(object):
    
    def __init__(self,instructions):
        self.inst = instructions # the L system string to be interpreted
        self.pts = [Point()]
        self.css = [CS()]
        self.angle = math.pi/4
        self.step_size = 1.0

    # moves the turtle based on the L System string
    def go(self):
        lines = []
        # while the length of the string is not 0
        while len(self.inst)&gt;0:
            chr = self.inst[0]
            
            # update the Coordinate Sytem based on the current string character
            if chr == '-': #yaw right
                self.do_xform(self.css[-1].zAxis,self.angle)
            elif chr == '+': #yaw left
                self.do_xform(self.css[-1].zAxis,-self.angle)
            elif chr == '^': #pitch up
                self.do_xform(self.css[-1].xAxis,-self.angle)
            elif chr == '&amp;': #pitch down
                self.do_xform(self.css[-1].xAxis,self.angle)
            elif chr == '}': #roll right
                self.do_xform(self.css[-1].yAxis,self.angle)
            elif chr == '{': # roll left
                self.do_xform(self.css[-1].yAxis,-self.angle)
                
            # draw a new line along the y axis of the Coordinate System
            elif chr == 'F':
                nxt_pt = self.pts[-1] + (self.css[-1].yAxis * self.step_size)
                lines.append(Segment(self.pts[-1],nxt_pt))
                self.pts[-1] = nxt_pt
  
            # save the last place to branch from to a list
            elif chr == '[': 
                self.push(self.pts[-1], self.css[-1])
            # remove that branch from the list once it has been drawn
            elif chr == ']': 
                self.pop() 
            self.inst = self.inst[1:]
        return lines

    # the transformation to apply to the Coordinate System
    def do_xform(self,axis,angle):
        xf = Xform.rotation(axis=axis,angle=angle)
        self.css[-1] = self.css[-1] * xf
        pass
    # adds the current point and CS to lists
    def push(self,pt,cs):
        self.pts.append(pt)
        self.css.append(cs)
    # removes the last point and CS from lists
    def pop(self):
        self.pts = self.pts[:-1]
        self.css = self.css[:-1]</item>
