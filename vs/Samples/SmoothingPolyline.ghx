                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // smoothens a polyline
    if (pl != null)                             //if pl is different than null
    {
      List&lt;Point3d&gt; pts = new List&lt;Point3d&gt;(pl); // get the polyline vertices

      if (pts.Count &gt; 0)
      {
        int lastPtIndex = pts.Count - 1;
        double oneLessFactor = 1 - factor;

        List&lt;Point3d&gt; newPts = pts;
        while (cycles-- &gt; 0)            // decreases at each cycle
        {
          newPts = new List&lt;Point3d&gt;(); // newPts becomes a new list

          for (int i = 0; i &lt; pts.Count; i++)
          {
            Point3d pt = pts[i];
            if (i == 0 || i == lastPtIndex)
            {
              newPts.Add(pt);
              continue;
            }

            Point3d newPt = (pts[i - 1] + pts[i + 1]) * 0.5;
            newPt = newPt * factor + pt * oneLessFactor;
            newPts.Add(newPt);
          }

          pts = newPts;                 // exchange pts with newPts
        }

        A = newPts;
      }
    }
</item>
