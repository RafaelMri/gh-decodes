              </items>
              <chunks count="1">
                <chunk name="Container">
                  <items count="10">
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    ' smoothens a polyline
    If pl IsNot Nothing Then
      'if pl is different than null
      Dim pts As New List(Of Point3d)(pl)

      ' get the polyline vertices
      If pts.Count &gt; 0 Then

        Dim lastPtIndex As Integer = pts.Count - 1
        Dim oneLessFactor As Double = 1 - factor

        Dim newPts As List(Of Point3d) = pts
        While cycles &gt; 0
          ' decreases at each cycle
          newPts = New List(Of Point3d)()

          ' newPts becomes a new list
          For i As Integer = 0 To pts.Count - 1
            Dim pt As Point3d = pts(i)

            If i = 0 OrElse i = lastPtIndex Then
              newPts.Add(pt)
              Continue For
            End If

            Dim newPt As Point3d = (pts(i - 1) + pts(i + 1)) * 0.5
            newPt = newPt * factor + pt * oneLessFactor
            newPts.Add(newPt)
          Next

          pts = newPts  ' exchange pts with newPts
          cycles -= 1   ' decreases cycles by one
        End While

        A = newPts
      End If
    End If
</item>
