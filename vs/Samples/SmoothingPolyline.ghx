                            <item name="string" type_name="gh_string" type_code="10"># smoothens a polyline (RhinoCommon)
from Rhino.Collections import Point3dList

if pl: #if pl is different than null
    
    pts = Point3dList(pl) # get the polyline vertices
    
    if len(pts) &gt; 0:
        lastPtIndex = len(pts) - 1
        oneLessFactor = 1 - factor
        
        newPts = pts
        while cycles &gt; 0: # decreases at each cycle
            newPts = [] # newPts becomes a new list
            
            for (i, pt) in enumerate(pts):
                if i == 0 or i == lastPtIndex:
                    newPts.append(pt)
                    continue
                
                newPt = (pts[i - 1] + pts[i + 1]) * 0.5
                newPt = newPt * factor + pt * oneLessFactor
                newPts.append(newPt)
            
            pts = newPts # exchange pts with newPts
            cycles -= 1 # decreases cycles by one
            
        a = newPts</item>
